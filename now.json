{
  "version": 2,
  "name": "aicoach-backend",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 